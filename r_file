library(plotly)

trace_0 <- c(2.429156726,  2.927803475, 2.434079925)
trace_1 <- c(2.568180653,  3.604012307, 4.161974405)
trace_2 <- c(2.399472181,  3.307851232, 3.890031147)
trace_3 <- c(2.167954378,  3.007299715, 3.447712434)

x <- c(2:4)

data <- data.frame(x, trace_0, trace_1, trace_2,trace_3)

p <- plot_ly(data, x = ~x, y = ~trace_0, name = 'Matriz 100x100', type = 'scatter', mode = 'lines+markers') %>%
  add_trace(y = ~trace_1, name = 'Matriz 500x500', mode = 'lines+markers') %>%
  add_trace(y = ~trace_2, name = 'Matriz 1000x1000', mode = 'lines+markers') %>%
  add_trace(y = ~trace_3, name = 'Matriz 1500x1500', mode = 'lines+markers') %>%
  layout(title = "Multiplicação de Matrizes (ijk)", 
       xaxis = list(title = "# Threads", dtick=1),
       yaxis = list (title = "Speed Up"))


library(plotly)

trace_0 <- c(1.976558033,	2.428805621,	2.758244681)
trace_1 <- c(1.985874477,	2.702595871,	3.12329477)
trace_2 <- c(1.950427086,	2.754387553,	3.199543121)
trace_3 <- c(1.912884616,	2.567961024,	2.862097554)

x <- c(2:4)

data <- data.frame(x, trace_0, trace_1, trace_2,trace_3)

p <- plot_ly(data, x = ~x, y = ~trace_0, name = 'Matriz 100x100', type = 'scatter', mode = 'lines+markers') %>%
  add_trace(y = ~trace_1, name = 'Matriz 500x500', mode = 'lines+markers') %>%
  add_trace(y = ~trace_2, name = 'Matriz 1000x1000', mode = 'lines+markers') %>%
  add_trace(y = ~trace_3, name = 'Matriz 1500x1500', mode = 'lines+markers') %>%
  layout(title = "Multiplicação de Matrizes (ijk -O3)", 
       xaxis = list(title = "# Threads", dtick=1),
       yaxis = list (title = "Speed Up"))





