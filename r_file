p <- plot_ly(data, x = ~x, y = ~trace_0, name = 'Matriz 100x100', type = 'scatter', mode = 'lines+markers') %>%
  add_trace(y = ~trace_1, name = 'Matriz 500x500', mode = 'lines+markers') %>%
